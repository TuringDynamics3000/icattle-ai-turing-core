# Local Development Environment Configuration
# Copy this file to .env for local development

# PostgreSQL Database (Docker)
DATABASE_URL=postgresql://icattle:icattle_dev_password@localhost:5432/icattle

# Application Settings
NODE_ENV=development
PORT=3000

# Owner OpenID (for admin access)
OWNER_OPEN_ID=your_openid_here

# Xero Integration (Optional - for testing)
XERO_CLIENT_ID=your_xero_client_id
XERO_CLIENT_SECRET=your_xero_client_secret
XERO_REDIRECT_URI=http://localhost:3000/api/xero/callback

# AgriWebb Integration (Optional - for testing)
AGRIWEBB_API_KEY=your_agriwebb_api_key
AGRIWEBB_API_SECRET=your_agriwebb_api_secret

# AWS S3 (Optional - for image uploads)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=ap-southeast-2
AWS_S3_BUCKET=icattle-images

# Kafka (Optional - for event streaming)
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=icattle-dashboard
