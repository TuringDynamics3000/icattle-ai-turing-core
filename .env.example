# Database Configuration
# PostgreSQL connection string for iCattle database
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://postgres:postgres_admin_password@localhost:5433/icattle

# Application Environment
NODE_ENV=development

# OAuth Configuration (Optional - for authentication features)
# OAUTH_SERVER_URL=https://your-oauth-server.com
# OAUTH_CLIENT_ID=your_client_id
# OAUTH_CLIENT_SECRET=your_client_secret

# Analytics (Optional)
# VITE_ANALYTICS_ENDPOINT=https://your-analytics-endpoint.com
# VITE_ANALYTICS_WEBSITE_ID=your_website_id
